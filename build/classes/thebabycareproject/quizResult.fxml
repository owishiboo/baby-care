<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="560.0" prefWidth="785.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="thebabycareproject.QuizResultController">
   <children>
      <ScrollPane prefHeight="200.0" prefWidth="200.0" stylesheets="@scrollpaneQuiz.css">
         <content>
            <VBox fx:id="vbox" alignment="CENTER_LEFT">
               <children>
                  <Label styleClass="fn-35" stylesheets="@homepage.css" text="Hey! Here is your score:">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <HBox alignment="TOP_CENTER" prefWidth="785.0">
                     <children>
                        <Label fx:id="score" stylesheets="@homepage.css">
                           <styleClass>
                              <String fx:value="fn-64" />
                              <String fx:value="text-font" />
                           </styleClass>
                        </Label>
                        <Label fx:id="label1" stylesheets="@homepage.css">
                           <styleClass>
                              <String fx:value="fn-64" />
                              <String fx:value="text-font" />
                           </styleClass>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </HBox>
                  <VBox alignment="TOP_CENTER" prefWidth="785.0">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                     <children>
                        <ImageView fx:id="imageView" fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefWidth="785.0">
                     <children>
                        <Label fx:id="opinion" styleClass="fn-35" stylesheets="@homepage.css">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" top="10.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </VBox>
                  <Label styleClass="fn-35" stylesheets="@homepage.css" text="Here are the answers:">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="answer" styleClass="fn-24" stylesheets="@homepage.css">
                     <VBox.margin>
                        <Insets bottom="10.0" left="30.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <VBox alignment="CENTER" prefWidth="785.0">
                     <children>
                        <JFXButton buttonType="RAISED" onAction="#goBack" prefHeight="46.0" prefWidth="202.0" ripplerFill="WHITE" style="-fx-background-color: #000000;" styleClass="fn-16" stylesheets="@homepage.css" text="Go Back to Menu" textFill="WHITE" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </VBox>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</StackPane>
